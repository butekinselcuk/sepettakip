{
  "name": "sepet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "seed": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/seed.ts",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:testlogin": "node scripts/test-login.js",
    "test:e2e": "playwright test tests/e2e/index.spec.ts --project=chromium",
    "test:e2e:admin": "playwright test tests/e2e/admin.spec.ts --project=chromium",
    "test:e2e:courier": "playwright test tests/e2e/courier.spec.ts --project=chromium",
    "test:e2e:business": "playwright test tests/e2e/business.spec.ts --project=chromium",
    "test:e2e:customer": "playwright test tests/e2e/customer.spec.ts --project=chromium",
    "test:e2e:auth": "playwright test tests/e2e/auth-system.spec.ts --project=chromium",
    "test:e2e:testlogin": "playwright test tests/e2e/test-login.spec.ts --project=chromium",
    "test:e2e:setup": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" tests/e2e/setup/test-data.ts",
    "test:e2e:report": "playwright show-report",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:test": "docker-compose --profile test up --abort-on-container-exit",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down -v",
    "preinstall": "npx only-allow npm",
    "staging:setup": "bash scripts/staging-setup.sh",
    "staging:setup:seed": "bash scripts/staging-setup.sh --with-seed",
    "staging:build": "NODE_ENV=staging next build",
    "staging:start": "NODE_ENV=staging next start",
    "staging:db:push": "dotenv -e staging.env -- npx prisma db push",
    "staging:db:studio": "dotenv -e staging.env -- npx prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node --require ts-node/register --loader ts-node/esm prisma/seed.ts",
    "test:auth": "node scripts/test-direct-auth.js",
    "test:login": "node --loader ts-node/esm scripts/run-login-tests.js",
    "update:auth": "node scripts/update-auth-system.js",
    "fix-database": "node scripts/fix-database.js",
    "test:comprehensive": "node scripts/test-comprehensive.js",
    "test:all-pages": "node scripts/test-all-routes.js",
    "run:seed": "node scripts/seed.js"
  },
